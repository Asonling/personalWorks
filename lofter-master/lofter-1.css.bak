*{
	margin:0;
	padding:0;
}
body{
	font-family:"Microsoft YaHei";
	font-size:12px;
	background:#EDEDEF;
}
ul,li{
	list-style:none;
}
img{
	border:none;
}
a{
	text-decoration:none;
	outline:none;
}
.clear{
	clear:both;
}
/*header start*/
.header{
	position:relative;
	margin:0 auto;
	width:100%;
	height:84px;
	background:#24292C;
	min-width:930px;
}
.container{
	position:relative;
	width:930px;
	margin:0 auto;
}
.logo{
	background:url("../lofter-master/images/hd24.png") no-repeat;
	position:relative;
	width:200px;
	height:44px;
	background-position:0 0;
	float:left;
	margin-top:20px;
}
.header-nav{
	position:relative;
	margin-left:177px;
	height:84px;
	float:left;
}
.header-nav li{
	float:none;
	position:relative;
	list-style:none;
	display:inline-block;
	padding-right:33px;
	font-size:16px;
	line-height:31px;
	margin-top:34px;
}		
.header-nav li.active a{
	color: #fff;
}
.header-nav li a{
	text-decoration:none;
	color:#999999;
}
.header a:hover{
	color:white;
}
.header .hearder-more-hover:hover .header-more{
	background-position:-22px 0px;
}
.header-arrow{
	background:url("../lofter-master/images/hd24.png") no-repeat;
}
.header-more,.search-btn,.search-btn2{
	background:url("../lofter-master/images/icon4-24.png") no-repeat;
}
.header-nav li .header-arrow{
	position:absolute;
	margin-bottom:0px;
	background-position:-118px -130px;
	height:20px;
	left:8px;
	width:20px;
	display:block;
}
.header-more{
	display:inline-block;
	height:10px;
	width:10px;
	margin-left:7px;
	background-position:0px 0px;
}
.header-search .search-box{
	position:relative;
	height:30px;
	line-height:30px;
	width:230px;
	border-radius:15px;
	background:#000000;
	float:left;
	margin-top:34px;
	overflow: hidden;
	
}
.search-box .search-btn{
	height:22px;
	width:22px;
	display:inline-block;
	top:5px;
	margin-left:10px;
	border:none;
	background-position:0 -76px;
}
.search-box .search-btn:hover{
	background-position:-62px -76px;
}
.search-box input{
	position:absolute;
	background:rgba(0,0,0,0);
	border:none;
	outline:none;
	height:22px;
	line-height:22px;
	color:#fff;
	top:3px;
}
.search-btn2{
	position:absolute;
	height:24px;
	right:6px;
	width:42px;
	display:inline-block;
	background-position:-210px -75px;

}


